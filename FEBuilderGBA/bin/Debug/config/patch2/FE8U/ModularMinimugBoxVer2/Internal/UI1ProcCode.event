		PROC_LOOP_ROUTINE(MMBSetup)
		PROC_LOOP_ROUTINE(MMBExtendWindow)
		PROC_LABEL(2)
		PROC_LOOP_ROUTINE(MMBCheckCursor)
		PROC_LABEL(3)
		PROC_LOOP_ROUTINE(MMBRetractWindow)
		PROC_GOTO(0)
		PROC_LABEL(1)
